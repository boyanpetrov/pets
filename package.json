    "lodash": "^4.17.5",
    "mocha": "^5.0.1",
    "mustache": "^2.3.0",
    "node-cron": "^1.2.1",
    "node-pg-migrate": "^2.21.0",
    "nodemon": "^1.14.12",
    "passport": "^0.4.0",
    "passport-bnet": "^1.2.1",
    "passport-local": "^1.0.0",
    "pg-promise": "^7.5.2"
